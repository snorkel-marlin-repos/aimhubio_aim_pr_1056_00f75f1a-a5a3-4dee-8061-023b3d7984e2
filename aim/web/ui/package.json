    "format:fix": "eslint src/. --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "analyze-bundles": "node tasks/bundle-analyzer.js",
    "crc-kit": "func() { node tasks/cli/index.js  create-component --name=\"$1\" --path=./src/components/kit/ --lint; }; func",
